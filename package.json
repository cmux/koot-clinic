        "koa-body": "^4.1.1",
        "koa-ejs": "^4.3.0",
        "koa-views": "^6.2.1",
        "koot": "^0.12.0-alpha.23",
        "koot-diagnose": "^0.13.3",
        "koot-redux": "^1.0.18",
        "marked": "^0.8.0",
        "md5": "^2.2.1",
        "moment": "^2.24.0",
        "mysql": "^2.17.1",
        "otplib": "^11.0.1",
        "pretty-ms": "^5.1.0",
        "qrcode": "^1.4.4"
