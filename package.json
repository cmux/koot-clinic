        "eslint-config-koot": "^1.6.0",
        "forever": "^1.0.0",
        "forever-monitor": "^1.7.1",
        "husky": "^3.0.9",
        "jest": "^24.9.0",
        "lint-staged": "^9.5.0",
        "normalize.css": "^8.0.1",
        "prettier": "^1.19.1",
        "prettier-eslint": "^9.0.0",
        "redux-devtools-extension": "^2.13.8",
        "redux-logger": "^3.0.6",
        "simple-git": "^1.129.0",
        "svg-url-loader": "^3.0.3",
        "url-loader": "^3.0.0"
    },
    "bugs": {
        "url": "https://github.com/cmux/koot-clinic/issues"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,cjs,mjs}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{json,md,css,less,sass,scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "koot": {
        "baseVersion": "0.12.0",
        "version": "0.12.0-alpha.23"
    }
}
